<table border="1">
    <tr>
        <th colspan="4">STUDENT LIST</th>
    </tr>
    <tr>
        <th>Student Name</th>
        <th>Student DoB</th>
        <th>Student Gender</th>
        <th>Student GPA</th>
    </tr>
    {{#each students }}
    <tr>
        <th>{{ name }}</th>
        <th>{{ dob }}</th>
        <th>{{ gender }}</th>
        <th>{{ gpa }}</th>
    </tr>
    {{/each}}
</table>